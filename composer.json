{
  "name" : "gesdinet/jwt-refresh-token-bundle",
  "description" : "Implements a refresh token system over Json Web Tokens in Symfony",
  "type" : "symfony-bundle",
  "authors" : [{
    "name" : "Marcos GÃ³mez Vilches",
    "email" : "marcos@gesdinet.com"
  }],
  "keywords" : [
    "jwt refresh token bundle symfony json web"
  ],
  "license" : [
    "MIT"
  ],
  "require" : {
    "php": ">=8.0",
    "lexik/jwt-authentication-bundle": "^2.0",
    "symfony/config": "^4.4|^5.3|^6.0",
    "symfony/console": "^4.4|^5.3|^6.0",
    "symfony/dependency-injection": "^4.4|^5.3|^6.0",
    "symfony/deprecation-contracts": "^2.1",
    "symfony/event-dispatcher": "^4.4|^5.3|^6.0",
    "symfony/http-foundation": "^4.4|^5.3|^6.0",
    "symfony/http-kernel": "^4.4|^5.3|^6.0",
    "symfony/polyfill-php80": "^1.15",
    "symfony/property-access": "^4.4|^5.3|^6.0",
    "symfony/security-bundle": "^3.4|^4.0|^5.2",
    "symfony/security-core": "^4.4|^5.3|^6.0",
    "symfony/security-http": "^4.4|^5.3|^6.0"
  },
  "require-dev": {
    "babdev/phpspec-skip-example-extension": "^1.0",
    "doctrine/cache": "^1.11|^2.0",
    "doctrine/mongodb-odm": "^2.3",
    "doctrine/persistence": "^1.3.3|^2.0",
    "doctrine/orm": "^2.7",
    "matthiasnoback/symfony-config-test": "^4.2",
    "matthiasnoback/symfony-dependency-injection-test": "^4.2",
    "phpspec/phpspec": "^7.0",
    "phpunit/phpunit": "^9.5",
    "symfony/cache": "^4.4|^5.3|^6.0",
    "symfony/security-guard": "^4.4|^5.3"
  },
  "conflict": {
    "doctrine/mongodb-odm": "<2.0",
    "doctrine/orm": "<2.7",
    "doctrine/persistence": "<1.3.3"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Gesdinet\\JWTRefreshTokenBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Gesdinet\\JWTRefreshTokenBundle\\spec\\": "spec/",
      "Gesdinet\\JWTRefreshTokenBundle\\Tests\\": "Tests/"
    }
  },
  "extra" : {
    "branch-alias" : {
      "dev-master" : "1.0-dev"
    }
  },
  "minimum-stability": "dev"
}
